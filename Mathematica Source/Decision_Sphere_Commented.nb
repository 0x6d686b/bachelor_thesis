   3.5389758893468733`*^9, 3.5389759290200977`*^9}},
 TextJustification->1.],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Ortho", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"x1_", ",", "y1_"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"x2_", ",", "y2_"}], "}"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"\[Theta]1", ",", "\[Phi]1", ",", "\[Theta]2", ",", "\[Phi]2"}],
       "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "\[Theta]1", ",", "\[Phi]1", ",", "\[Theta]2", ",", "\[Phi]2"}], 
        "}"}], "=", 
       RowBox[{
        FractionBox["\[Pi]", "180"], 
