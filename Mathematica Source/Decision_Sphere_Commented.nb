Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"DynProgr", "[", "Start_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"Grph", ",", "ProgrDyn"}], "}"}], ",", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"ProgrDyn", "[", "slice_", "]"}], ":=", 
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"Spread", ",", "Etabli", ",", "Posi"}], "}"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"Spread", "=", "4"}], ";", "\[IndentingNewLine]", 
          RowBox[{"Do", "[", "\[IndentingNewLine]", "\t", 
           RowBox[{
            RowBox[{
             RowBox[{"Etabli", "=", 
              RowBox[{"Table", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"Grph", "[", 
                  RowBox[{"[", 
                   RowBox[{
                    RowBox[{"slice", "-", "1"}], ",", "j", ",", "2"}], "]"}], 
                  "]"}], "+", 
                 RowBox[{"Ortho", "[", 
                  RowBox[{
                   RowBox[{"Loc", "[", 
                    RowBox[{"[", 
                    RowBox[{
                    RowBox[{"slice", "-", "1"}], ",", "j"}], "]"}], "]"}], 
                   ",", 
                   RowBox[{"Loc", "[", 
                    RowBox[{"[", 
                    RowBox[{"slice", ",", "k"}], "]"}], "]"}]}], "]"}]}], ",", 
                RowBox[{"{", 
                 RowBox[{"j", ",", "1", ",", "Maxj"}], "}"}]}], "]"}]}], ";", 
             "\[IndentingNewLine]", "\t", "\[IndentingNewLine]", "\t", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"{", "Posi", "}"}], "}"}], "=", 
              RowBox[{"Position", "[", 
               RowBox[{"Etabli", ",", 
                RowBox[{"Min", "[", "Etabli", "]"}]}], "]"}]}], ";", 
             "\[IndentingNewLine]", "\t", "\[IndentingNewLine]", "\t", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"Abs", "[", 
                 RowBox[{"k", "-", "Posi"}], "]"}], "\[LessEqual]", 
                "Spread"}], ",", 
               RowBox[{
                RowBox[{"Grph", "[", 
                 RowBox[{"[", 
                  RowBox[{"slice", ",", "k"}], "]"}], "]"}], "=", 
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"slice", "-", "1"}], ",", "Posi"}], "}"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"slice", ",", "k"}], "}"}]}], "}"}], ",", 
                  RowBox[{"Min", "[", "Etabli", "]"}]}], "}"}]}]}], "]"}]}], 
            ",", 
            RowBox[{"{", 
             RowBox[{"k", ",", "1", ",", "Maxj"}], "}"}]}], "]"}]}]}], 
        "\[IndentingNewLine]", "\[IndentingNewLine]", "\t", "]"}]}], ";", " ", 
      RowBox[{"(*", " ", 
       RowBox[{"end", " ", "ProgrDyn"}], " ", "*)"}], "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"DynProgr", " ", "starts", " ", "here"}], " ", ";"}], "*)"}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"Grph", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"0", ",", 
           SuperscriptBox["10", "6"]}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "Maxi"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "1", ",", "Maxj"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Grph", "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "Start"}], "]"}], "]"}], "=", 
       RowBox[{"{", 
        RowBox[{"1", ",", "0"}], "}"}]}], ";", 
      StyleBox["\[IndentingNewLine]", "Subsubsection"], 
